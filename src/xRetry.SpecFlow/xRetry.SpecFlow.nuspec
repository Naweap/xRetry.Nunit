<?xml version="1.0"?>
<package>
  <metadata>
    <id>xRetry.SpecFlow.NUnit</id>
    <version>1.0.6</version>
    <authors>Josh Keegan, Beau Gosse</authors>
    <owners>Beau Gosse</owners>
    <projectUrl>https://github.com/JoshKeegan/xRetry</projectUrl>
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Allows for a @retry(n) tag to be placed above scenarios in SpecFlow.NUnit. This can be used to re-run a failed test up to n times, allowing for transient failures (e.g. network related) to be tolerated. Based on Josh Keegan's xRetry.SpecFlow package</description>
    <copyright>2019</copyright>
    <tags>SpecFlow NUnit Retry Test-Automation</tags>

    <dependencies>
      <dependency id="SpecFlow" version="3.0.220" />
    </dependencies>
  </metadata>

  <files>
    <file src="xRetry.SpecFlow.targets" target="build" />
    <file src="xRetry.SpecFlow.props" target="build" />

    <file src="bin\Release\net471\xRetry.SpecFlowPlugin.dll" target="build\net471" />
    <file src="bin\Release\netstandard2.0\xRetry.SpecFlowPlugin.dll" target="build\netstandard2.0" />
  </files>
</package>